# -*-shell-script-*-

PACKAGE=guile-dbd-sqlite3

DBD_MAJOR_VERSION=2
DBD_MINOR_VERSION=1
DBD_MICRO_VERSION=6

DBD_EFFECTIVE_VERSION=${DBD_MAJOR_VERSION}.${DBD_MINOR_VERSION}
DBD_VERSION=${DBD_EFFECTIVE_VERSION}.${DBD_MICRO_VERSION}
VERSION=${DBD_VERSION}

DBD_ITF_CURRENT=${DBD_MAJOR_VERSION}
DBD_ITF_REVISION=${DBD_MICRO_VERSION}
DBD_ITF_AGE=0
DBD_INTERFACE="${DBD_ITF_CURRENT}:${DBD_ITF_REVISION}:${DBD_ITF_AGE}"
